{
  "name": "what-to-watch",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://dmborozdin.github.io/What-to-Watch/",
  "description": "Личный проект «What-to-watch» от HTML Academy",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server",
    "eslint": "eslint --ext .jsx --ext .js src/",
    "test": "npm run eslint && npm run test.jest",
    "test.jest": "jest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public"
  },
  "devDependencies": {
    "@babel/core": "7.12.17",
    "@babel/preset-env": "7.12.17",
    "@babel/preset-react": "7.12.13",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "5.1.3",
    "@testing-library/user-event": "12.8.3",
    "axios-mock-adapter": "1.19.0",
    "babel-loader": "8.2.3",
    "css-loader": "5.2.7",
    "eslint": "7.18.0",
    "eslint-config-htmlacademy": "2.1.0",
    "eslint-plugin-react": "7.22.0",
    "gh-pages": "^4.0.0",
    "jest": "26.6.3",
    "redux-mock-store": "1.5.4",
    "style-loader": "2.0.0",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.5.0",
    "axios": "0.21.4",
    "dayjs": "1.10.6",
    "history": "4.10.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.6",
    "react-router-dom": "5.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/style-mock.js"
    },
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ]
  }
}
